End Sub

'Poner las imagenes de la viborita...segun correspondan
Sub PonerLaImagendelaVibora()
'Vemos la anterior y las posterior
For a = 2 To LargoVibora - 1

    If (SnakePos(a - 1).X = SnakePos(a).X) And (SnakePos(a + 1).X <> SnakePos(a).X) Then
        'Vemos si es arriba o abajo
        
        If SnakePos(a - 1).Y > SnakePos(a).Y And SnakePos(a + 1).X < SnakePos(a).X Then Snake(a - 1).Picture = Picture3(11).Picture
        If SnakePos(a - 1).Y > SnakePos(a).Y And SnakePos(a + 1).X > SnakePos(a).X Then Snake(a - 1).Picture = Picture3(10).Picture
        
        If SnakePos(a - 1).Y < SnakePos(a).Y And SnakePos(a + 1).X < SnakePos(a).X Then Snake(a - 1).Picture = Picture3(8).Picture
        If SnakePos(a - 1).Y < SnakePos(a).Y And SnakePos(a + 1).X > SnakePos(a).X Then Snake(a - 1).Picture = Picture3(9).Picture
    End If
    
    If (SnakePos(a - 1).Y = SnakePos(a).Y) And (SnakePos(a + 1).Y <> SnakePos(a).Y) Then
        'Vemos si es arriba o abajo
        
        If SnakePos(a - 1).X > SnakePos(a).X And SnakePos(a + 1).Y < SnakePos(a).Y Then Snake(a - 1).Picture = Picture3(9).Picture
        If SnakePos(a - 1).X > SnakePos(a).X And SnakePos(a + 1).Y > SnakePos(a).Y Then Snake(a - 1).Picture = Picture3(10).Picture
        
        If SnakePos(a - 1).X < SnakePos(a).X And SnakePos(a + 1).Y < SnakePos(a).Y Then Snake(a - 1).Picture = Picture3(8).Picture
        If SnakePos(a - 1).X < SnakePos(a).X And SnakePos(a + 1).Y > SnakePos(a).Y Then Snake(a - 1).Picture = Picture3(11).Picture
    End If

    'Se mueve por las Y
    If (SnakePos(a - 1).X = SnakePos(a).X) And (SnakePos(a + 1).X = SnakePos(a).X) Then Snake(a - 1).Picture = Picture3(2).Picture
    'Se mueve por las X
    If (SnakePos(a - 1).Y = SnakePos(a).Y) And (SnakePos(a + 1).Y = SnakePos(a).Y) Then Snake(a - 1).Picture = Picture3(0).Picture
   
    
Next
'Ahora ponemos la cola
Snake(LargoVibora - 1).Picture = Picture3(1).Picture

End Sub

'Pone el Bonus en la pantalla
Sub CalcularBonus()
Randomize Timer
Dim Bx          As Integer
Dim By          As Integer
Dim OkL         As Boolean
Dim OKV         As Boolean
Do
    OKV = False
    If Nivel <> 1 Then
        OkL = False
    Else
        OkL = True
    End If
    Bx = (6630 * Rnd) + 1
    By = (5865 * Rnd) + 1
    Bx = Int(Bx / 255) * 255
    By = Int(By / 255) * 255
    'Vemos que no caiga sobre la vibora
    For a = 1 To LargoVibora
        If Bx = SnakePos(a).X And By = SnakePos(a).Y Then
            OKV = False
            Exit For
        Else
            OKV = True
        End If
    Next
    If OKV = True Then
        'vemos que no caiga sobre un ladrillo
        If Nivel <> 1 Then
            For a = 1 To MaxLadrillos
                If Bx = Ladrillos(a).X And By = Ladrillos(a).Y Then
                    'callo sobre uno
                    OkL = False
                    Exit For
                Else
                    'No callo
                    OkL = True
                End If
            Next
        End If
    End If
Loop Until OkL = True And OKV = True
Pollito.Left = Bx
Pollito.Top = By
End Sub
'Dibuja la Vibora en Pantalla
Sub DibujarVibora()
For a = 1 To LargoVibora
    Snake(a - 1).Left = SnakePos(a).X
    Snake(a - 1).Top = SnakePos(a).Y
Next
End Sub

'Recalculamos las posiciones de la vibora
Sub RecalcularPos()
'El objetivo es poner a cada cuadro la posicion del cuadro anterior
Dim PosAnt1     As PosViBora
Dim PosAnt2     As PosViBora
'Primero obtenemos la posicion anterior
PosAnt2.X = SnakePos(1).X
PosAnt2.Y = SnakePos(1).Y


If Izquierda = True Then Snake(0).Picture = Picture3(5).Picture
If Derecha = True Then Snake(0).Picture = Picture3(4).Picture
If Arriba = True Then Snake(0).Picture = Picture3(6).Picture
If Abajo = True Then Snake(0).Picture = Picture3(7).Picture
    
For a = 2 To LargoVibora
    'Tomo la pos 2
    'Veemos si la posicion de la cabeza es igual a la del cuerpo

    PosAnt1.X = SnakePos(a).X
    PosAnt1.Y = SnakePos(a).Y

    SnakePos(a).X = PosAnt2.X
    SnakePos(a).Y = PosAnt2.Y

    PosAnt2.X = PosAnt1.X
    PosAnt2.Y = PosAnt1.Y

Next

SnakePos(1).X = NuevaPos.X
SnakePos(1).Y = NuevaPos.Y
End Sub

'Calcular si se Comio el Pollito
Function SeloComio() As Boolean
If Snake(0).Left = Pollito.Left Then
    If Snake(0).Top = Pollito.Top Then
        SeloComio = True
    Else
        SeloComio = False
    End If
Else
    SeloComio = False
End If
End Function

'Calcular el CHOQUE
Function SeChoco() As Boolean
For a = 2 To LargoVibora - 1
    If Snake(a).Left = Snake(0).Left Then
        If Snake(a).Top = Snake(0).Top Then
            SeChoco = True
            MsgBox "Se comio solo"
            Exit For
        Else
            SeChoco = False
        End If
    Else
        SeChoco = False
    End If
Next

If Snake(0).Left < 0 Then
    SeChoco = True
    Exit Function
End If
If Snake(0).Top < 0 Then
    SeChoco = True
    Exit Function
End If
If Snake(0).Left > 6630 Then
    SeChoco = True
    Exit Function
End If
If Snake(0).Top > 5865 Then
    SeChoco = True
    Exit Function
End If

If Nivel <> 1 Then
    'ahora vemos que no choco con los ladrillos
    For a = 1 To MaxLadrillos
        If SnakePos(1).X = Ladrillos(a).X And SnakePos(1).Y = Ladrillos(a).Y Then
            SeChoco = True
            MsgBox "Se choco la pared"
            Exit For
        End If
    Next
End If
End Function

